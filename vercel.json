{
  "builds": [
    {
      "src": "ejercicio2/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ejercicio1",
      "dest": "/api/ejercicio1.ts"
    },
    {
      "src": "/ejercicio2/(.*)",
      "dest": "ejercicio2/$1"
    },
    {
      "src": "/api/ejercicio1",
      "dest": "/api/ejercicio1.ts"
    }
  ]
}